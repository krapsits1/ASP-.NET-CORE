
@{
    ViewData["Title"] = "Courses";
}

<h1>Courses</h1>


<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Teacher</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Name</td>
                <td>@course.Teacher.Name</td>
            </tr>
        }
    </tbody>
</table>

